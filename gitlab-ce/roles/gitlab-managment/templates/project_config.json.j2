{
  "name": "{{ project_name }}",
  "path": "{{ project_path | default(project_name) }}",
  "description": "{{ project_description | default('Project created via Ansible') }}",
  "visibility": "{{ project_visibility | default('public') }}",
  "initialize_with_readme": "{{ initialize_with_readme | default(false) }}",
  "default_branch": "{{ default_branch | default('main') }}",
  {% if namespace_id is defined %}
  "namespace_id": {{ namespace_id }},
  {% endif %}
  "container_registry_enabled": {{ enable_container_registry | default(false) | to_json }}
}